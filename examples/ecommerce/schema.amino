# E-commerce Schema for Pricing Rules

struct customer {
    id: str,
    tier: str,
    purchase_count: int,
    total_spent: float,
    signup_date: str
}

struct product {
    id: str,
    category: str,
    price: float,
    inventory_count: int,
    brand: str
}

struct order {
    id: str,
    total: float,
    item_count: int,
    shipping_zip: str,
    date: str
}

# Custom functions available in rules
calculate_discount: (tier_discount: float, volume_discount: float) -> float
is_holiday_season: (date: str) -> bool
get_shipping_cost: (zip_code: str, weight: float) -> float