# E-commerce Schema for Pricing Rules

struct customer {
    id: str,
    tier: str,
    purchase_count: int,
    total_spent: float,
    signup_date: str
}

struct product {
    id: str,
    category: str,
    price: float,
    inventory_count: int,
    brand: str
}

struct order {
    id: str,
    total: float,
    item_count: int,
    shipping_zip: str,
    date: str
}

# Custom functions available in rules
calculate_discount: (float, float) -> float
is_holiday_season: (str) -> bool
get_shipping_cost: (str, float) -> float