# E-commerce Schema for Pricing Rules

struct customer {
    id: Str,
    tier: Str,
    purchase_count: Int,
    total_spent: Float,
    signup_date: Str
}

struct product {
    id: Str,
    category: Str,
    price: Float,
    inventory_count: Int,
    brand: Str
}

struct order {
    id: Str,
    total: Float,
    item_count: Int,
    shipping_zip: Str,
    date: Str
}

# Custom functions available in rules
calculate_discount: (tier_discount: Float, volume_discount: Float) -> Float
is_holiday_season: (date: Str) -> Bool
get_shipping_cost: (zip_code: Str, weight: Float) -> Float